		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper h1 {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<form action="">
			<table>
				<tr>
					<td>
						<label>Email:</label>
					</td>
					<td><input name="email" type="email"/></td>
				</tr>
				<tr>
					<td>
						<label>Password:</label>
					</td>
					<td><input name="password" type="password"/></td>
				</tr>
				<tr>
					<td><input type="submit" value="Submit"/></td>
				</tr>
			</table>
		</form>
	</div>
{% endblock %}
